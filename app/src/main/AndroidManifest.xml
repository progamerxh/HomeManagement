<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tkgd.homemanagement">

    <application
        android:allowBackup="true"
        android:icon="@drawable/home_plus"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="tkgd.homemanagement.Activity.ScenarioActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="tkgd.homemanagement.Activity.RoomActivity" />
        <activity android:name="tkgd.homemanagement.Activity.NotificationActivity" />
        <activity android:name="tkgd.homemanagement.Activity.NewScenarioActivity" />
        <activity android:name="tkgd.homemanagement.Activity.NewRoomActivity" />
        <activity android:name="tkgd.homemanagement.Activity.NewDeviceActivity" />
        <activity android:name="tkgd.homemanagement.Activity.DeviceActivity" />
    </application>

</manifest>